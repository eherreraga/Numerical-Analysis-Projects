function [x] = GaussSeidel(A, b, x0, error)
%GaussSeidel Method:
% x^(k + 1) = D^-1(b-Rx^(k))
%   A - Its going to be the matrix
%   b - right side vector
%   x0 - initial strating approximation
%   error - error tolerance

clc;

[R, C] = size(A);

if R ~= C
    error('A is not a square matrix...')
end

 %Iterative process
 while err > error
     x0(1,1) = 1/9*(b(1,1) - x0(2,1) - x0(3,1));
     x0(2) = 1/10*(b(2) - 2*x0(1) - 3*x0(3));
     x0(3) = 1/11*(b(3) - 3*x0(1) - 4*x0(2));
 end
 
 
 
 
  x = x0;

end

